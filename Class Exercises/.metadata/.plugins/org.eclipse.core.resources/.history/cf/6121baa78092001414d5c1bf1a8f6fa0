package com.course.animals;

public class TestAnimals {
	public static void main(String[] args) {
//		Animal a = new Animal("Ani", 8);
//		System.out.println(a.toString());
		
		
		Dolphin dolf1 = new Dolphin("Dolphi", 8, 10);
		Dolphin dolf2 = new Dolphin();
		
		
		System.out.println(dolf2.toString());
		
		Hedgehog hed = new Hedgehog("Kipi", 7654, 765);
		
		Dog d = new Dog();
		
//		
    	System.out.println(hed.toString());
    	
		doSomethingWithAnimal(dolf1);
		doSomethingWithAnimal(d);
		doSomethingWithAnimal(hed);
		
		
	}
	
	public static void doSomethingWithAnimal(Animal a)
	{
		a.eat();
	}
}
